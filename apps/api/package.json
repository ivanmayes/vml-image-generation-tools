{
	"name": "vml-starter-api",
	"version": "1.0.0",
	"description": "Starter API",
	"author": "Michael May",
	"license": "MIT",
	"scripts": {
		"prebuild": "node ./tools/clear-dist.js",
		"build": "nest build",
		"prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
		"console": "node dist/console.js",
		"db:migrations:build": "tsc -p tsconfig.migrations.json",
		"db:migrations:writeConfig": "node ./tools/create-ormconfig.js",
		"db:migrations:create": "npm run db:migrations:writeConfig && ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d ./ormconfig.ts",
		"db:migrations:run": "npm run db:migrations:writeConfig && ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d ./ormconfig.ts",
		"db:migrations:revert": "npm run db:migrations:writeConfig && ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d ./ormconfig.ts",
		"start": "node dist/main",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"useEnv:local": "node ./tools/use-env.js local",
		"useEnv:dev": "node ./tools/use-env.js dev",
		"useEnv:prod": "node ./tools/use-env.js prod"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.32.0",
		"@aws-sdk/client-bedrock-runtime": "^3.700.0",
		"@aws-sdk/client-sesv2": "^3.879.0",
		"@google/genai": "^1.40.0",
		"@google/generative-ai": "^0.21.0",
		"@nestjs/axios": "^4.0.1",
		"@nestjs/common": "^11.1.6",
		"@nestjs/core": "^11.1.6",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-express": "^11.1.6",
		"@nestjs/swagger": "^11.2.0",
		"@nestjs/throttler": "^6.4.0",
		"@nestjs/typeorm": "^11.0.0",
		"@sendgrid/mail": "^8.1.5",
		"@wavezync/nestjs-pgboss": "^5.1.1",
		"aws-sdk": "^2.1692.0",
		"axios": "^1.11.0",
		"bcrypt": "^6.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"colors": "^1.4.0",
		"compression": "^1.8.1",
		"cookie-parser": "^1.4.7",
		"cron-parser": "^5.3.1",
		"dotenv": "^17.2.2",
		"express": "^5.1.0",
		"express-rate-limit": "^8.0.1",
		"express-sslify": "^1.2.0",
		"handlebars": "^4.7.8",
		"helmet": "^8.1.0",
		"jszip": "^3.10.1",
		"jwk-to-pem": "^2.0.7",
		"mammoth": "^1.11.0",
		"nestjs-console": "^10.0.0",
		"nodemailer": "^7.0.6",
		"openai": "^4.77.0",
		"passport": "^0.7.0",
		"passport-http-bearer": "^1.0.1",
		"passport-jwt": "^4.0.1",
		"pdf-parse": "^2.4.5",
		"pg": "^8.16.3",
		"pg-boss": "^8.4.2",
		"pgvector": "^0.2.1",
		"rxjs": "^7.8.2",
		"swagger-ui-express": "^5.0.1",
		"typeorm": "^0.3.27",
		"uuid": "^11.1.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@darraghor/eslint-plugin-nestjs-typed": "^7.1.0",
		"@eslint/js": "^9.39.2",
		"@nestjs/cli": "^11.0.10",
		"@nestjs/schematics": "^11.0.7",
		"@nestjs/testing": "^11.1.6",
		"@types/aws-sdk": "^2.7.4",
		"@types/bcrypt": "^6.0.0",
		"@types/compression": "^1.8.1",
		"@types/express": "^5.0.3",
		"@types/express-serve-static-core": "^5.0.7",
		"@types/express-sslify": "^1.2.5",
		"@types/jest": "30.0.0",
		"@types/jwk-to-pem": "^2.0.3",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.3.0",
		"@types/nodemailer": "^7.0.1",
		"@types/passport-http-bearer": "^1.0.42",
		"@types/passport-jwt": "^4.0.1",
		"@types/serve-static": "^1.15.8",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "8.42.0",
		"@typescript-eslint/parser": "8.42.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-sonarjs": "^3.0.6",
		"jest": "^30.1.3",
		"minimist": "^1.2.8",
		"prettier": "^3.6.2",
		"reflect-metadata": "^0.2.2",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.1",
		"ts-loader": "^9.5.4",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"tslint": "6.1.3",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.54.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
