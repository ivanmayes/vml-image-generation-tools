{
	"moduleFileExtensions": ["js", "json", "ts"],
	"rootDir": ".",
	"testEnvironment": "node",
	"testRegex": ".e2e-real-spec.ts$",
	"transform": {
		"^.+\\.(t|j)s$": "ts-jest"
	},
	"moduleNameMapper": {
		"^src/(.*)$": "<rootDir>/../src/$1"
	},
	"setupFiles": ["<rootDir>/e2e-env-setup.ts"],
	"globalSetup": "<rootDir>/global-setup.ts",
	"globalTeardown": "<rootDir>/global-teardown.ts"
}
